@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":TAbsParser" as TAbsParser LOGIC_COLOR
participant ":DeleteTutorialCommandParser" as DeleteTutorialCommandParser LOGIC_COLOR
participant "d:DeleteTutorialCommand" as DeleteTutorialCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("delete t/T1")
activate LogicManager

LogicManager -> TAbsParser : parseCommand("delete t/T1")
activate TAbsParser

create DeleteTutorialCommandParser
TAbsParser -> DeleteTutorialCommandParser
activate DeleteTutorialCommandParser

DeleteTutorialCommandParser --> TAbsParser
deactivate DeleteTutorialCommandParser

TAbsParser -> DeleteTutorialCommandParser : parse("T1")
activate DeleteTutorialCommandParser

create DeleteTutorialCommand
DeleteTutorialCommandParser -> DeleteTutorialCommand
activate DeleteTutorialCommand

DeleteTutorialCommand --> DeleteTutorialCommandParser :
deactivate DeleteTutorialCommand

DeleteTutorialCommandParser --> TAbsParser : d
deactivate DeleteTutorialCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
DeleteTutorialCommandParser -[hidden]-> TAbsParser
destroy DeleteTutorialCommandParser

TAbsParser --> LogicManager : d
deactivate TAbsParser

LogicManager -> DeleteTutorialCommand : execute(m)
activate DeleteTutorialCommand

DeleteTutorialCommand -> Model : deleteTutorial(T1)
activate Model

Model --> DeleteTutorialCommand
deactivate Model

create CommandResult
DeleteTutorialCommand -> CommandResult
activate CommandResult

CommandResult --> DeleteTutorialCommand
deactivate CommandResult

DeleteTutorialCommand --> LogicManager : r
deactivate DeleteTutorialCommand

[<--LogicManager
deactivate LogicManager
@enduml
